{
  "expo": {
    "name": "v2m",
    "slug": "v2m",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/v2mlogo.png",
    "splash": {
      "image": "./assets/images/v2mlogo.png",
      "resizeMode": "contain",
      "backgroundColor": "#000000"
    },
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "ios": {
        "config": {
          "googleMapsApiKey": "AIzaSyDXFs1aEjvPeX_k1FZdv94qNEVdNor6N6c"
        }
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "config": {
        "googleMaps": {
          "apiKey": "AIzaSyA2q6-WiPlmfrXSBlCuykiHFnzJDXPQxow"
        }
      },
      "package": "com.hassan.v2m"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "e3e9623e-7728-49ac-9dbb-f9606b7936ca"
      }
    },
    "owner": "pri3sh"
  }
}
