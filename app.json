{
  "expo": {
    "name": "v2m",
    "slug": "v2m",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/v2mlogo.png",
    "splash": {
      "image": "./assets/images/v2mlogo.png",
      "resizeMode": "contain",
      "backgroundColor": "#000000"
    },
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "ios": {
        "config": {
          "googleMapsApiKey": "AIzaSyDOKkabnKbWNVc8QYZmEgotd1RrViVzg8Q"
        }
      }

    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "config": {
        "googleMaps": {
          "apiKey": "AIzaSyDOKkabnKbWNVc8QYZmEgotd1RrViVzg8Q"
        }
      }
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    }
  }
}
